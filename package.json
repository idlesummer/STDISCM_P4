{
  "name": "stdiscm-p4",
  "version": "1.0.0",
  "private": true,
  "description": "STDISCM Machine Learning Project",
  "scripts": {
    "gen": "npm run gen-py && npm run gen-ts",
    "gen-py": "cd backend && uv run python -m grpc_tools.protoc -I ../packages/proto --python_out=src/proto --grpc_python_out=src/proto --pyi_out=src/proto ../packages/proto/metrics.proto",
    "gen-ts": "cd frontend && node ../node_modules/grpc-tools/bin/protoc.js --js_out=import_style=commonjs,binary:src/proto --grpc_out=grpc_js:src/proto --plugin=protoc-gen-grpc=../node_modules/.bin/grpc_tools_node_protoc_plugin -I ../packages/proto ../packages/proto/metrics.proto && node ../node_modules/grpc-tools/bin/protoc.js --plugin=protoc-gen-ts=../node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:src/proto -I ../packages/proto ../packages/proto/metrics.proto"
  },
  "workspaces": [
    "frontend",
    "backend"
  ]
}
